{
    "model": {
        "path": "network_attack_detector_quantized.onnx",
        "metadata_path": "model_metadata.pkl"
    },
    "monitoring": {
        "log_file": "logs/attack_log.json",
        "batch_size": 32,
        "alert_threshold": 0.8
    },
    "performance": {
        "max_inference_time_ms": 100,
        "target_throughput": 100
    },
    "alerts": {
        "enable_email": false,
        "enable_syslog": true,
        "enable_file_log": true
    }
}
